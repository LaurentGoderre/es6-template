{
  "name": "demo",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist/*",
    "copy": "cp -r src/i18n dist/",
    "build": "npm run test && npm run copy && npx rollup -c && npm run uglify",
    "uglify": "npx uglifyjs-folder dist --each --output dist",
    "test": "npm run test-lint",
    "test-lint": "npx eslint . --ignore-pattern 'node_modules/*' --ignore-pattern 'dist/*'",
    "prebuild": "npm run clean",
    "prepare": "npm run build"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "eslint": "^5.7.0",
    "eslint-config-google": "^0.11.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "uglifyjs-folder": "^1.5.1"
  }
